NUXT_PUBLIC_SUPABASE_URL=https://<your-project-id>.supabase.co
NUXT_PUBLIC_SUPABASE_ANON_KEY=<your-anon-key>

# ==================================================================
# Private Runtime Configuration (Server-Only)
# ==================================================================
SUPABASE_SERVICE_ROLE_KEY=<your-service-role-key>

