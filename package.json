{
  "name": "genai-photobooth",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "db:pull": "prisma db pull",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "test:clearCache": "jest --clearCache",
    "test:coverage": "jest --selectProjects server --selectProjects app --coverage",
    "test:coverage:server": "jest --selectProjects server --coverage",
    "test:coverage:app": "jest --selectProjects app --coverage",
    "test:all": "jest --selectProjects server --selectProjects app", 
    "test:setup": "jest --selectProjects setup --runInBand",
    "test:server": "jest --selectProjects server --runInBand",
    "test:app": "jest --selectProjects app --runInBand",
    "test:file": "powershell -Command \"$FILE='tests/server/oauth.test.ts'; jest $FILE\""
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@iconify/vue": "^5.0.0",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.2",
    "@nuxtjs/supabase": "^1.6.1",
    "@prisma/client": "^6.16.1",
    "@supabase/supabase-js": "^2.57.0",
    "babel-jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "nuxt": "^4.0.3",
    "sharp": "^0.34.4",
    "typescript": "^5.9.2",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^27.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.1",
    "ts-jest": "^29.4.4"
  }
}
